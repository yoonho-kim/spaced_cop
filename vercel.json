{
    "crons": [
        {
            "path": "/api/volunteer-auto-lottery",
            "schedule": "0 0 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
